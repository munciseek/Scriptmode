		pivotcloney.Parent = workspace.CurrentRooms[room]
		pivotcloney:PivotTo(r.LightFixture.CFrame)
		pivotcloney.CFrame = pivotcloney.CFrame - Vector3.new(0, 6, 0)
		pivotcloney.Transparency = 1
		r:Destroy()
	end
end
